{
	"geode": "4.10.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "ramble21.stopdyingthere",
	"name": "StopDyingThere",
	"version": "v1.0.1",
	"developer": "Ramble21",
	"description": "Plays lines from Cold's iconic video whenever you die in GD",
	"repository": "https://github.com/Ramble21/stopdyingthere",
	"links": {
		"source": "https://github.com/Ramble21/stopdyingthere"
	},
	"dependencies": {
		"geode.node-ids": ">=v1.20.0"
	},
	"settings": {
		"enable": {
			"type": "bool",
			"default": true,
			"name": "Enable",
			"description": "Enables the mod"
		},
		"disableinpractice": {
			"type": "bool",
			"default": true,
			"name": "Disable in practice mode",
			"description": "Disables the mod while playing in practice mode or from a StartPos"
		},
		"bestonly": {
			"type": "bool",
			"default": false,
			"name": "Play on Best Only",
			"description": "Only play sound effects when dying at/past your normal mode best"
		},
		"levelcomplete": {
			"type": "bool",
			"default": true,
			"name": "Play on level completion",
			"description": "Plays Cold's celebration when you beat a level"
		},
		"volume": {
			"type": "int",
			"default": 25,
			"min": 0,
			"max": 100,
			"name": "Volume",
			"description": "Set the mod's volume (0-100%)- sfx are VERY LOUD so low volume recommended"
		}
	},
	"resources": {
